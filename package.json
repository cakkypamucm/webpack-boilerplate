{
    "name": "webpack-boilerplate-cakkypamucm",
    "version": "1.0.0",
    "private": true,
    "author": "Denis S. <cakkypamucm@yandex.ru>",
    "type": "module",
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack serve --mode=development",
        "build": "webpack --mode=production",
        "lint": "run-s eslint stylelint prettier",
        "eslint": "eslint . --cache --fix",
        "stylelint": "stylelint \"src/**/*.{css,scss,vue}\" --cache --fix",
        "prettier": "prettier . \"!(package)*.json\" --cache --cache-location .prettiercache --find-config-path --write",
        "typecheck": "vue-tsc --noEmit",
        "build-browserslist-regexp": "(echo export default && browserslist-useragent-regexp --ignoreMinor --allowHigherVersions) > src/utils/supported-browsers-regexp.ts"
    },
    "dependencies": {
        "@fontsource/roboto-slab": "^5.1.0",
        "core-js": "^3.40.0",
        "normalize.css": "^8.0.1",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-typescript": "^7.26.0",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.2.1",
        "browserslist": "^4.24.2",
        "browserslist-useragent-regexp": "^4.1.3",
        "copy-webpack-plugin": "^12.0.2",
        "critters-webpack-plugin": "github:cakkypamucm/critters-webpack-plugin",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "dotenv": "^16.4.7",
        "eslint": "^8.57.0",
        "eslint-config-cakkypamucm": "github:cakkypamucm/eslint-config",
        "eslint-config-vue-typescript-cakkypamucm": "github:cakkypamucm/eslint-config-vue-typescript",
        "eslint-import-resolver-webpack": "^0.13.8",
        "html-webpack-plugin": "^5.6.3",
        "mini-css-extract-plugin": "^2.9.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.49",
        "postcss-loader": "^8.1.1",
        "postcss-preset-env": "8.3.0",
        "prettier": "^3.5.0",
        "prettier-plugin-sh": "^0.14.0",
        "sass": "^1.82.0",
        "sass-loader": "^16.0.4",
        "style-loader": "^3.3.4",
        "stylelint": "16.12.0",
        "stylelint-config-vue-cakkypamucm": "github:cakkypamucm/stylelint-config-vue",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "typescript": "^5.7.2",
        "vue-loader": "^17.4.2",
        "vue-tsc": "^2.2.0",
        "webpack": "^5.97.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "webpackbar": "^7.0.0"
    },
    "engines": {
        "node": ">=18.18"
    }
}
