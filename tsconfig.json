{
    "extends": "@vue/tsconfig/tsconfig.json",
    "compilerOptions": {
        "incremental": true,
        "tsBuildInfoFile": ".tsbuildcache",
        "isolatedModules": true,
        "allowJs": true,
        "sourceMap": false,
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src/**/*.ts", "src/**/*.vue"]
}
